-- üîß Utility function
local function webhookLog(text)
    pcall(function()
        HttpService:PostAsync(webhook, HttpService:JSONEncode({ content = text }))
    end)
end

-- üì¶ Create player panel entry
local function addUser(player)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -10, 0, 60)
    frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    frame.BorderSizePixel = 0
    frame.Name = player.Name

    local name = Instance.new("TextLabel", frame)
    name.Size = UDim2.new(0.3, 0, 1, 0)
    name.Text = player.Name
    name.Font = Enum.Font.Gotham
    name.TextSize = 16
    name.TextColor3 = Color3.fromRGB(255, 255, 255)
    name.BackgroundTransparency = 1
    name.TextXAlignment = Enum.TextXAlignment.Left
    name.Position = UDim2.new(0, 5, 0, 0)

    local function makeButton(text, color, callback)
        local btn = Instance.new("TextButton", frame)
        btn.Size = UDim2.new(0, 60, 0, 22)
        btn.BackgroundColor3 = color
        btn.Text = text
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 13
        btn.AutoButtonColor = true
        btn.MouseButton1Click:Connect(callback)
        return btn
    end

    -- Buttons
    local x = 0.32
    local function place(btn)
        btn.Position = UDim2.new(x, 0, 0.1, 0)
        x += 0.13
    end

    place(makeButton("Kick", Color3.fromRGB(200, 50, 50), function()
        player:Kick("You have been kicked by a BaconHub admin.")
        webhookLog("üü• "..LocalPlayer.Name.." kicked "..player.Name)
    end))

    place(makeButton("Freeze", Color3.fromRGB(80, 100, 255), function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.Anchored = true
            webhookLog("üßä "..LocalPlayer.Name.." froze "..player.Name)
        end
    end))

    place(makeButton("Unfreeze", Color3.fromRGB(50, 255, 100), function()
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.Anchored = false
            webhookLog("‚úÖ "..LocalPlayer.Name.." unfroze "..player.Name)
        end
    end))

    place(makeButton("FakeBan", Color3.fromRGB(120, 0, 0), function()
        player:Kick("You have been permanently banned from BaconHub.\nReason: Unauthorized behavior detected.")
        webhookLog("‚õî "..LocalPlayer.Name.." fake-banned "..player.Name)
    end))

    place(makeButton("Msg", Color3.fromRGB(255, 200, 0), function()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
            Text = "[BaconHub] Hello from Admin!",
            Color = Color3.fromRGB(255, 255, 0)
        })
        webhookLog("üí¨ "..LocalPlayer.Name.." sent message to "..player.Name)
    end))

    frame.Parent = scroll
end

-- üë• Populate users
for _, p in pairs(Players:GetPlayers()) do
    if p ~= LocalPlayer then addUser(p) end
end

Players.PlayerAdded:Connect(function(p)
    if p ~= LocalPlayer then
        addUser(p)
    end
end)

Players.PlayerRemoving:Connect(function(p)
    local entry = scroll:FindFirstChild(p.Name)
    if entry then entry:Destroy() end
end)
